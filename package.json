{
  "name": "@campus-explorer/aws-credentials",
  "description": "JavaScript helper code to manage AWS credentials",
  "version": "2.4.0",
  "author": "Stephen Caldwell <steve@campusexplorer.com>",
  "bugs": "https://github.com/campus-explorer/aws-credentials-js/issues",
  "dependencies": {
    "inquirer": "^7.3.3",
    "lodash": "^4.17.20",
    "memoizee": "^0.4.14",
    "mkdirp": "^1.0.4"
  },
  "devDependencies": {
    "@types/inquirer": "^7.3.1",
    "@types/lodash": "^4.14.160",
    "@types/memoizee": "^0.4.4",
    "@types/mkdirp": "^1.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "fixpack": "^3.0.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "loud-rejection": "^2.2.0",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2",
    "xo": "^0.33.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/src"
  ],
  "homepage": "https://github.com/campus-explorer/aws-credentials-js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "main": "src/index.js",
  "peerDependencies": {
    "aws-sdk": "2.x"
  },
  "repository": "campus-explorer/aws-credentials-js",
  "scripts": {
    "lint": "xo",
    "posttest": "yarn lint",
    "test": "true",
    "ts:check": "tsc",
    "ts:watch": "tsc --watch"
  },
  "types": "src/index.d.ts",
  "xo": {
    "prettier": true,
    "rules": {
      "capitalized-comments": "off",
      "no-warning-comments": "off",
      "ava/no-todo-test": "off",
      "eslint-comments/no-unused-disable": "off"
    }
  }
