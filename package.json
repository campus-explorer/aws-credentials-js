{
  "name": "@campus-explorer/aws-credentials",
  "description": "JavaScript helper code to manage AWS credentials",
  "version": "2.3.0",
  "author": "Stephen Caldwell <steve@campusexplorer.com>",
  "bugs": "https://github.com/campus-explorer/aws-credentials-js/issues",
  "dependencies": {
    "aws-sdk": "^2.670.0",
    "inquirer": "^7.1.0",
    "lodash": "^4.17.15",
    "memoizee": "^0.4.14",
    "mkdirp": "^1.0.4"
  },
  "devDependencies": {
    "@types/inquirer": "^6.5.0",
    "@types/lodash": "^4.14.150",
    "@types/memoizee": "^0.4.4",
    "@types/mkdirp": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "fixpack": "^3.0.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "loud-rejection": "^2.2.0",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3",
    "xo": "^0.31.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/src"
  ],
  "homepage": "https://github.com/campus-explorer/aws-credentials-js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "main": "src/index.js",
  "repository": "campus-explorer/aws-credentials-js",
  "scripts": {
    "lint": "xo",
    "posttest": "yarn lint",
    "test": "true",
    "ts:check": "tsc",
    "ts:watch": "tsc --watch"
  },
  "types": "src/index.d.ts",
  "xo": {
    "prettier": true,
    "rules": {
      "capitalized-comments": "off",
      "no-warning-comments": "off",
      "ava/no-todo-test": "off",
      "eslint-comments/no-unused-disable": "off"
    }
  }
